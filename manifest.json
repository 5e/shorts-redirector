{
    "manifest_version": 2,
    "name": "Shorts Redirector",
    "version": "1.0",
  
    "description": "Converts YouTube 'shorts' to normal videos",
  
    "icons": {
      "48": "icons/icon.svg"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["main.js"]
      }
    ]
  }
  