{
    "manifest_version": 2,
    "name": "YouTube Shorts Redirector",
    "version": "1.0",
  
    "description": "Converts YouTube shorts to normal videos",
  
    "icons": {
      "48": "icons/icon.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["main.js"]
      }
    ]
  }
  